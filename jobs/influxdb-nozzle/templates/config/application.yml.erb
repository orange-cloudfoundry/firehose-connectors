---
logging:
  level: DEBUG

management:
  context-path: /admin    

spring:
  sleuth:
    sampler:
      percentage: <%= p('zipkin.sampler-percentage') %>
  zipkin:
    enabled: <%= p('zipkin.enabled') %>
    base-url: <%= p('zipkin.base-url') %> 

admin:
  user:  <%= p('admin.user') %>
  password: <%= p('admin.password') %>     

  
firehose:
  url: <%= p('firehose.url') %>
  bearer: <%= p('firehose.bearer') %>
  skipTlsValidation: <%= p('firehose.skipTlsValidation') %>
  